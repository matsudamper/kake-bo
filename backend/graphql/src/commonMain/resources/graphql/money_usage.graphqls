type SuggestMoneyUsage {
    title: String!
    description: String!
    date: LocalDateTime
    price: Int
    type: MoneyUsageCategory
    service: MoneyUsageService
}

type MoneyUsageCategory {
    id: MoneyUsageCategoryId!
    name: String!
}

type MoneyUsageSubCategory {
    id: MoneyUsageSubCategoryId!
    name: String!
    largeCategory: MoneyUsageCategory!
}

type MoneyUsageService {
    id: MoneyUsageServiceId!
    name: String!
}
