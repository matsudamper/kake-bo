type User {
    isLoggedIn: Boolean!
    settings: UserSettings! @lazy
    userMailAttributes: UserMailAttributes!
    importedMailAttributes: ImportedMailAttributes!
}

type MoneyUsage {
    id: MoneyUsageId!
    title: String!
    description: String!
    date: LocalDateTime!

    moneyUsageService: MoneyUsageService
    moneyUsageSubCategory: MoneyUsageSubCategory
}

