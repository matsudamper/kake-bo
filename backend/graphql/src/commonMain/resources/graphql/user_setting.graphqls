type UserSettings {
    imapConfig: UserImapConfig @lazy
    fidoAddInfo: FidoAddInfo! @lazy
    registeredFidoList: [RegisteredFidoInfo!]! @lazy
}

type RegisteredFidoInfo {
    id: FidoId!
    name: String!
    base64CredentialId: String!
}

type UserImapConfig {
    host: String
    port     :Int
    userName :String
    hasPassword :Boolean
}
